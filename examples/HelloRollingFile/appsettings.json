{
  "Logging": {
    "RollingFile": {
      "FilePathTemplate": "Log-{DateTime:yyyyMMdd-HH}.log",
      "IncludeScopes": true,
      "Template": "{LocalDateTime:yyyy-MM-dd'T'HH':'mm':'sszzz} {ShortLevel}: {CategoryName}[{Id:0000}]{ScopeList}\r\n      {Message} (thread {ThreadId}) {Exception}"
    },
    "LogLevel" : {
      "Default": "Trace",
      "Microsoft": "Warning"
    }
  }
}